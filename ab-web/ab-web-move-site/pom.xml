    <build>
        <finalName>ab-web-move-site</finalName>
         <plugins>
			<plugin>
				<groupId>org.apache.tomcat.maven</groupId>
				<artifactId>tomcat7-maven-plugin</artifactId>
				<version>2.1</version>
				<configuration>
					<port>9003</port>
					<path>/ab-web-move-site</path>
					<uriEncoding>UTF-8</uriEncoding>
					<finalName>ab-web-move-site</finalName>
					<server>tomcat7</server>
				</configuration>
			</plugin>
		</plugins>
    </build>
</project>