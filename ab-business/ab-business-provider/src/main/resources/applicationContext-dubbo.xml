    <bean id="hjsApproveSmsService" class="com.cifex.business.provider.system.approve.HjsApproveSmsServiceImpl"></bean>
    <bean id="hjsApproveSmsSendlogService" class="com.cifex.business.provider.system.approve.HjsApproveSmsSendlogServiceImpl"></bean>
    
	
    
    
	<!-- 管理后台配置结束 -->
	
