	<dubbo:service interface="com.cifex.business.api.user.user.UsersInfoService" ref="usersInfoService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.user.user.UsersLogService" ref="usersLogService" protocol="dubbo" />
	<dubbo:service interface="com.cifex.business.api.user.message.UsersMessageService" ref="usersMessageService" protocol="dubbo"/>
	<dubbo:service interface="com.cifex.business.api.user.user.UsersAddService" ref="usersAddService" protocol="dubbo"/>
	
	<dubbo:service interface="com.cifex.business.api.user.approve.ApproveRealnameService" ref="approveRealnameService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.user.approve.ApproveEmailActiveService" ref="approveEmailActiveService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.user.approve.ApproveSmsService" ref="approveSmsService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.user.approve.ApproveSmsSendlogService" ref="approveSmsSendlogService"  protocol="dubbo"  />

	
	<bean id="sysAreaService" class="com.cifex.business.provider.user.area.SysAreaServiceImpl"></bean>
	<bean id="sysConfigService" class="com.cifex.business.provider.user.config.SysConfigServiceImpl"></bean>
	
	<bean id="userBaseService" class="com.cifex.business.provider.user.account.UserBaseServiceImpl"></bean>
	<bean id="userService" class="com.cifex.business.provider.user.user.UserServiceImpl"></bean>
    <bean id="usersInfoService" class="com.cifex.business.provider.user.user.UsersInfoServiceImpl"></bean>
    <bean id="usersLogService" class="com.cifex.business.provider.user.user.UsersLogServiceImpl"></bean>
	<bean id="usersAddService" class="com.cifex.business.provider.user.user.UsersAddServiceImpl"></bean>
	<bean id="usersMessageService" class="com.cifex.business.provider.user.message.UsersMessageServiceImpl"></bean>
    
    <bean id="approveRealnameService" class="com.cifex.business.provider.user.approve.ApproveRealnameServiceImpl"></bean>
    <bean id="approveEmailActiveService" class="com.cifex.business.provider.user.approve.ApproveEmailActiveServiceImpl"></bean>
    <bean id="approveSmsService" class="com.cifex.business.provider.user.approve.ApproveSmsServiceImpl"></bean>
    <bean id="approveSmsSendlogService" class="com.cifex.business.provider.user.approve.ApproveSmsSendlogServiceImpl"></bean>
	<!-- 前台配置结束 -->
	
</beans> 