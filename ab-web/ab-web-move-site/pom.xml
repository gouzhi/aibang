                <redis.ip>127.0.0.1</redis.ip>
                <redis.port>6379</redis.port>
                <activemq.url>tcp://120.25.169.233:61616/</activemq.url>

                <ip.ipseeker.ip.file>D://qqwry.dat</ip.ipseeker.ip.file>
                <css.image.domain>/ab-web-move-site/static</css.image.domain>
                <web.domain>.domain.com.cn</web.domain>
                <web.name>m.aibang</web.name>
                <project.name>/ab-web-move-site</project.name>
                <web.commoninterceptor.image.domain.public>http://172.11.1.249/</web.commoninterceptor.image.domain.public>
                <web.commoninterceptor.image.domain>http://172.11.1.249/</web.commoninterceptor.image.domain>
                <sms.flag>off</sms.flag>
                <product.type>1</product.type>
            </properties>
            <activation>
