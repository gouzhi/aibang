                <ip.ipseeker.ip.file>/data/huafax/qqwry.dat</ip.ipseeker.ip.file>
                <css.image.domain>/static</css.image.domain>
                <web.domain>.huafax.com</web.domain>
                <web.name>m-test</web.name>
                <project.name></project.name>
                <web.commoninterceptor.image.domain.public>http://172.11.1.249/</web.commoninterceptor.image.domain.public>
                <web.commoninterceptor.image.domain>http://172.11.1.249/</web.commoninterceptor.image.domain>
                <sms.flag>off</sms.flag>
                <product.type>1</product.type>
            </properties>
        </profile>
        <profile>
            <id>production</id>
            <properties>
                <zookeeper.url>10.45.139.237:2181</zookeeper.url>
                <memcache.ip>10.45.139.237</memcache.ip>
                <memcache.port>11211</memcache.port>
                <redis.ip>10.45.139.237</redis.ip>
                <redis.port>6379</redis.port>
                <activemq.url>tcp://10.45.136.93:61616</activemq.url>

