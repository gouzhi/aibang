	<dubbo:service interface="com.cifex.business.api.system.user.HjsUsersOptLogService" ref="hjsUsersOptLogService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.user.HjsUsersOptService" ref="hjsUsersOptService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.user.HjsUsersInfoService" ref="hjsUsersInfoService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.user.HjsUsersLogService" ref="hjsUsersLogService" protocol="dubbo" />
	<dubbo:service interface="com.cifex.business.api.system.user.HjsUsersBlackService" ref="hjsUsersBlackService" protocol="dubbo" />
	
	<dubbo:service interface="com.cifex.business.api.system.approve.HjsApproveRealnameService" ref="hjsApproveRealnameService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.approve.HjsApproveEmailActiveService" ref="hjsApproveEmailActiveService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.approve.HjsApproveSmsService" ref="hjsApproveSmsService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.approve.HjsApproveSmsSendlogService" ref="hjsApproveSmsSendlogService"  protocol="dubbo"  />
	
	
    <bean id="hjsSysMenuService" class="com.cifex.business.provider.system.menu.HjsSysMenuServiceImpl"></bean>
    <bean id="hjsSysRoleService" class="com.cifex.business.provider.system.role.HjsSysRoleServiceImpl"></bean>
    <bean id="hjsSysUserRoleService" class="com.cifex.business.provider.system.userRole.HjsSysUserRoleServiceImpl"></bean>
    <bean id="hjsSysConfigService" class="com.cifex.business.provider.system.config.HjsSysConfigServiceImpl"></bean>
	<bean id="hjsSysAreaService" class="com.cifex.business.provider.system.area.HjsSysAreaServiceImpl"></bean>
   
    <bean id="hjsUserBaseService" class="com.cifex.business.provider.system.account.HjsUserBaseServiceImpl"></bean>
    <bean id="hjsUserService" class="com.cifex.business.provider.system.user.HjsUserServiceImpl"></bean>
    <bean id="hjsUsersOptLogService" class="com.cifex.business.provider.system.user.HjsUsersOptLogServiceImpl"></bean>
	<bean id="hjsUsersOptService" class="com.cifex.business.provider.system.user.HjsUsersOptServiceImpl"></bean>
