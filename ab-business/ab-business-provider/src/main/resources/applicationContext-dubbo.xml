<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
	
	<dubbo:application name="core-web-provider"  />

	<dubbo:registry address="zookeeper://${ZOOKEEPER_SERVER}" />
 
	<!-- 用dubbo协议在20880端口暴露服务 -->
    <dubbo:protocol name="dubbo" port="20880" threads="500"/>
    <dubbo:provider protocol="dubbo" default="true" timeout="350000"  />
	
	<!-- 管理后台配置 -->
	<dubbo:service interface="com.cifex.business.api.system.menu.HjsSysMenuService" ref="hjsSysMenuService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.role.HjsSysRoleService" ref="hjsSysRoleService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.userRole.HjsSysUserRoleService" ref="hjsSysUserRoleService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.config.HjsSysConfigService" ref="hjsSysConfigService"  protocol="dubbo"  />
	<dubbo:service interface="com.cifex.business.api.system.area.HjsSysAreaService" ref="hjsSysAreaService" protocol="dubbo" />
	
	<dubbo:service interface="com.cifex.business.api.system.account.HjsUserBaseService" ref="hjsUserBaseService"  protocol="dubbo"  />
